{
  "private": false,
  "name": "@antv/xflow",
  "version": "2.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "license": "MIT",
  "scripts": {
    "clean:turbo": "rimraf .turbo",
    "clean:build": "rimraf dist es lib",
    "clean:coverage": "rimraf ./test/coverage",
    "clean": "run-p clean:**",
    "build:esm": "tsc --module esnext --target es6 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es6 --outDir ./lib",
    "build:umd": "rollup -c --bundleConfigAsCjs",
    "build:dev": "run-p -s build:cjs build:esm",
    "build:watch": "run-s -s build:watch:esm",
    "build:watch:esm": "run-s -s 'build:esm -w'",
    "build:watch:cjs": "run-s -s 'build:cjs -w'",
    "build": "run-p -s build:dev build:umd",
    "prebuild": "run-s -s clean:build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@antv/x6": "2.0.0-beta.1",
    "@antv/x6-common": "^2.0.0",
    "@antv/x6-plugin-clipboard": "^2.0.0",
    "@antv/x6-plugin-dnd": "^2.0.0",
    "@antv/x6-plugin-export": "^2.0.0",
    "@antv/x6-plugin-history": "^2.0.0",
    "@antv/x6-react-shape": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "antd": "^4.24.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "antd": "^4.24.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
